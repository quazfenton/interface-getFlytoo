{
  "compilerOptions": {
    "target": "es2016",
    "jsx": "react-jsx",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
  },
  "include": ["**/*.ts", "**/*.tsx", "ui/**/*.ts", "ui/**/*.tsx"],
  "exclude": ["node_modules"]
}